{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build", "postinstall"],
      "outputs": ["dist/**", ".output/**"]
    },
    "postinstall": {
      "outputs": []
    },
    "typecheck": {
      "dependsOn": ["nuxt:prepare", "^typecheck"],
      "outputs": []
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "start": {
      "cache": false,
      "persistent": true
    }
  }
}
